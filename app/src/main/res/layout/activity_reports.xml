<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.openimis.imispolicies.tz.co.exact.Reports"
    android:orientation="vertical">

    <Button
        android:id="@+id/SnapshotIndicators"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:text="@string/SnapshotIndicators"/>
    <Button
        android:id="@+id/CumulativeIndicators"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:text="@string/CumulativeIndicators"/>

</LinearLayout>
